#{ticket.title} by #{current_user.longname}

<br>
<div>
#{t('Ticket URL')}: <a href="#{config.http_type}://#{config.fqdn}/#ticket/zoom/#{ticket.id}" target="zammad_app">#{config.http_type}://#{config.fqdn}/#ticket/zoom/#{ticket.id}</a><br>
#{t('Group')}: #{ticket.group.name}<br>
#{t('State')}: #{t(ticket.state.name)}<br>
</div>
<br>
<% if @objects[:changes].present? %>
  <div>
  #{t('Changes')}:<br>
  <% @objects[:changes].each do |key, value| %>
    <%= t key %>: <%= h value[0] %> -&gt; <%= h value[1] %><br>
  <% end %>
  </div>
<% end %>
<br>
<% if @objects[:article] %>
  <div>
  <blockquote type="cite">
  #{article.body_as_html}
  </blockquote>
  </div>
<% end %>
<br>
